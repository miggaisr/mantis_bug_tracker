  "properties": {
    "isViewingIssues": {
      "type": "identifier",
      "selector": "body",
      "control": "exists",
      "selectContains": "Viewing Issues"
    },
    "issueslist": {
      "type": "array",
      "selectorAll": "#buglist tr[bgcolor]",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 3
          },
          "category": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 6
          },
          "severity": {
            "type": "string",
            "selector": "a",
            "control": "action.link"
          },
          "summary": {
            "type": "string",
            "selector": "a",
            "control": "action.link"
          }
        }
      }
    }
  },
  "connect": []
}
